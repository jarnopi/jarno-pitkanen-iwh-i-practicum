doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Objects Table
        p
            a(href='/update-cobj') Add/Update this table
        br
        table(cellspacing=4)
            thead
                tr 
                    th(style="background-color: salmon;") Name
                    th(style="background-color: salmon;") Manufacturer 
                    th(style="background-color: salmon;") Model 
            tbody
                each car in data 
                    tr 
                        td 
                            - var id = car.hs_object_id;
                            a(href="/update-cobj?id=" + id) #{car.name}
                        td #{car.manufacturer}
                        td #{car.model}